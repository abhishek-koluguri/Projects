package application;

public class FibonacciRecursive extends FibonacciNumber {


    public int fibonacciValueAtAPosition(int position) {

        if(position < 0)
            return 0;
        if(position < 2)
            return 1;

        return fibonacciValueAtAPosition(position - 1) + fibonacciValueAtAPosition(position - 2);
    }
}
