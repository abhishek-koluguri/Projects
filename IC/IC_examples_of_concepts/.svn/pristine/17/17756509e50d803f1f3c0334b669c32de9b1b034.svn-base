var max = function(a, b) {
  if(arguments.length > 0) {
    var large = arguments[0];
    for(var i = 0; i < arguments.length; i++) {
      if(large < arguments[i])
        large = arguments[i];
    }
  }
  
  return large;
}

console.log(max(1, 2));
console.log(max(3, 2));

console.log(max(1, 4, 3));

console.log(max(1, 2, 5, 2));
console.log(max());